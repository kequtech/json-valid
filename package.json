{
  "name": "@kequtech/json-valid",
  "description": "JSON validator",
  "version": "0.1.0",
  "type": "module",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/main.js"
    }
  },
  "types": "./dist/main.d.ts",
  "files": [
    "dist",
    "README.md",
